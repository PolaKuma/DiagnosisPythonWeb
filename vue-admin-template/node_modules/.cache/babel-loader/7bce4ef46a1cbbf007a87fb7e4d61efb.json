AHA;QAIAC,sBAJA;QAKAC,cALA;QAMAC;MANA;MAQA;IACA,CAvBA;IAwBAe,MAxBA,oBAwBA;MAAA;;MAAA;QAAA;QAAA;UAAA;YAAA;cAAA;gBAAA;gBAAA,OAEA,mEAFA;;cAAA;gBAEAJ,GAFA;;gBAAA,MAIAA,gBAJA;kBAAA;kBAAA;gBAAA;;gBAKA;gBACA,iCANA,CAOA;;gBACA;kBACAK,gBADA;kBAEAC;gBAFA;;gBARA;gBAAA,OAYA,uDAZA;;cAAA;cAAA;gBAAA;YAAA;UAAA;QAAA;MAAA;IAcA,CAtCA;IAuCAC,OAvCA,mBAuCAL,GAvCA,EAuCA;MAAA;;MACA;QACAM,uBADA;QAEAC,sBAFA;QAGAH;MAHA,GAIAI,IAJA,0EAIA;QAAA;QAAA;UAAA;YAAA;cAAA;gBAAA;gBAAA,OACA,gCADA;;cAAA;gBACAC,MADA;;gBAAA,MAEAA,mBAFA;kBAAA;kBAAA;gBAAA;;gBAAA,eAGA,MAHA;gBAAA,eAGA;kBACAL,eADA;kBAEAD;gBAFA,CAHA;gBAAA;gBAAA,OAOA,qFAPA;;cAAA;gBAAA;;gBAAA,aAGAO,OAHA;;gBAAA;gBAAA;;cAAA;gBAUA;kBACAN,eADA;kBAEAD;gBAFA;;cAVA;cAAA;gBAAA;YAAA;UAAA;QAAA;MAAA,CAJA,IAmBAQ,KAnBA,CAmBA;QACA;UACAP,YADA;UAEAD;QAFA;MAIA,CAxBA;IAyBA,CAjEA;IAkEAS,gBAlEA,4BAkEAC,KAlEA,EAkEA;MACA;MACA;IACA,CArEA;IAsEAC,OAtEA,qBAsEA;MACA;MACA;QACA/B,YADA;QAEAK,YAFA;QAGAJ,YAHA;QAIAC,YAJA;QAKAC,QALA;QAMAC;MANA;IAQA,CAhFA;IAiFA4B,QAjFA,oBAiFAf,GAjFA,EAiFA;MACA;MACA;QACAjB,sBADA;QAEAE,sBAFA;QAGAE;MAHA;IAKA;EAxFA;AA1BA","names":["name","data","tableData","dialogFormVisible","infoDialogVisible","form","username","usertype","realname","desc","studentno","password","formLabelWidth","pageSize","pageNum","total","loading","mounted","methods","getUserList","pager","res","showForm","row","id","onForm","message","type","deleted","confirmButtonText","cancelButtonText","then","result","$notify","catch","handleSizeChange","limit","AddForm","showInfo"],"sourceRoot":"src/views/bookManage","sources":["user.vue"],"sourcesContent":["<template>\n  <div>\n    <el-button type=\"primary\" @click=\"AddForm\">增加人员</el-button>\n    <el-table :data=\"tableData\" v-loading=\"loading\" style=\"width: 100%\">\n      <el-table-column type=\"index\" label=\"序号\" align=\"center\" width=\"70\"/>\n      <el-table-column prop=\"studentno\" label=\"学号\" align=\"center\" width=\"180\"/>\n      <el-table-column prop=\"username\" label=\"登录名\" align=\"center\" width=\"180\"/>\n      <el-table-column prop=\"realname\" label=\"姓名\" align=\"center\" width=\"180\"/>\n      <el-table-column prop=\"usertype\" label=\"所属角色组\" align=\"center\" width=\"180\">\n        <template v-slot=\"{row}\">\n          <el-tag :type=\"row.usertype === 1 ? '' : 'danger' \" effect=\"dark\">\n            {{ row.usertype === 1 ? \"阴性\" : \"阳性\" }}\n          </el-tag>\n        </template>\n      </el-table-column>\n      <el-table-column prop=\"desc\" label=\"专业\" align=\"center\"/>\n      <el-table-column prop=\"date_joined\" label=\"日期\" align=\"center\"/>\n      <el-table-column label=\"操作\" align=\"center\" width=\"300\">\n        <template v-slot=\"{row}\">\n          <el-button type=\"primary\" @click=\"showForm(row)\" size='mini'>编辑</el-button>\n          <el-button type=\"danger\" @click=\"deleted(row)\" size='mini'>删除</el-button>\n          <el-button type=\"info\" @click=\"showInfo(row)\" size='mini'>显示</el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n    <el-pagination\n      background\n      style=\"margin-top: 20px;text-align: center\"\n      :current-page=\"pageNum\"\n      :page-size=\"pageSize\"\n      :total=\"total\"\n      :pager-count=\"5\"\n      :page-sizes=\"[5,15,30]\"\n      layout=\"prev,pager,next,jumper,->,sizes,total\"\n      @current-change=\"getUserList\"\n      @size-change=\"handleSizeChange\"\n    />\n\n    <el-dialog :title=\"form.id ? '修改用户':'新增用户'\" :visible.sync=\"dialogFormVisible\">\n      <el-form :model=\"form\">\n        <el-form-item label=\"学号\" :label-width=\"formLabelWidth\">\n          <el-input v-model=\"form.studentno\" autocomplete=\"off\" style=\"width: 300px;\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"姓名\" :label-width=\"formLabelWidth\">\n          <el-input v-model=\"form.realname\" autocomplete=\"off\" style=\"width: 300px;\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"登录名\" :label-width=\"formLabelWidth\">\n          <el-input v-model=\"form.username\" autocomplete=\"off\" style=\"width: 300px;\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"登陆密码\" :label-width=\"formLabelWidth\">\n          <el-input v-model=\"password\" type=\"password\" placeholder=\"默认密码：123456\" autocomplete=\"off\"\n                    style=\"width: 300px;\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"所属角色组\" :label-width=\"formLabelWidth\">\n          <el-select v-model=\"form.usertype\" placeholder=\"请选择权限\">\n            <el-option label=\"阴性\" value=\"1\"></el-option>\n            <el-option label=\"阳性\" value=\"2\"></el-option>\n          </el-select>\n        </el-form-item>\n        <el-form-item label=\"专业\" :label-width=\"formLabelWidth\">\n          <el-select v-model=\"form.desc\" placeholder=\"请选择性别\">\n            <el-option label=\"网络工程\" value=\"网络工程\"></el-option>\n            <el-option label=\"软件工程\" value=\"软件工程\"></el-option>\n            <el-option label=\"计算机科学与技术\" value=\"计算机科学与技术\"></el-option>\n            <el-option label=\"土木工程\" value=\"土木工程\"></el-option>\n            <el-option label=\"应用化学\" value=\"应用化学\"></el-option>\n          </el-select>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"onForm\">确 定</el-button>\n      </div>\n    </el-dialog>\n    <!-- Information dialog -->\n    <el-dialog :title=\"'用户信息'\" :visible.sync=\"infoDialogVisible\">\n      <!-- Display information without input fields -->\n      <el-form :model=\"form\" label-width=\"80px\">\n        <el-form-item label=\"学号\">\n          <el-input v-model=\"form.studentno\" :disabled=\"true\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"登录名\">\n          <el-input v-model=\"form.username\" :disabled=\"true\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"姓名\">\n          <el-input v-model=\"form.realname\" :disabled=\"true\"></el-input>\n        </el-form-item>\n      </el-form>\n\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"infoDialogVisible = false\">关闭</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"user\",\n  data() {\n    return {\n      tableData: [],\n      dialogFormVisible: false,\n      infoDialogVisible: false,\n      form: {\n        username: '',\n        usertype: '',\n        realname: '',\n        desc: '',\n        studentno: ''\n      },\n      password: '',\n      formLabelWidth: '120px',\n      pageSize: 5,\n      pageNum: 1,\n      total: 0,\n      loading: false,\n\n    }\n  },\n  mounted() {\n    this.getUserList()\n  },\n  methods: {\n    async getUserList(pager = 1) {\n      this.loading = true\n      this.pageNum = pager\n      const {pageNum, pageSize} = this // 发送请求时候需要带参数\n      const res = await this.$API.user.reqUser(pageNum, pageSize)\n      if (res.code === 200) {\n        this.tableData = res.msg\n        this.total = res.total\n        this.loading = false\n      }\n    },\n    showForm(row) {\n      this.dialogFormVisible = true\n      this.form = {\n        id: row.id,\n        username: row.username,\n        usertype: row.usertype.toString(),\n        realname: row.realname,\n        desc: row.desc,\n        studentno: row.studentno,\n      }\n      this.password = ''\n    },\n    async onForm() {\n\n      const res = await this.$API.user.reqSaveAndUpdateUser(this.form, this.password)\n\n      if (res.code === 200) {\n        // 关闭表单\n        this.dialogFormVisible = false\n        // 消息提示\n        this.$message({\n          message: '添加成功!',\n          type: 'success'\n        });\n        await this.getUserList(this.form.id ? this.pageNum : 1)\n      }\n    },\n    deleted(row) {\n      this.$confirm(`你确定删除 ${row.username} 吗?`, '删除', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(async () => {\n        const result = await this.$API.user.delUser(row.id)\n        if (result.code === 200) {\n          this.$notify({\n              type: 'success',\n              message: '删除成功!'\n            },\n            await this.getUserList(this.tableData.length > 1 ? this.pageNum : this.pageNum - 1)\n          )\n        } else {\n          this.$notify({\n            type: 'warning',\n            message: '删除失败,请联系系统运维!'\n          })\n        }\n      }).catch(() => {\n        this.$notify({\n          type: 'info',\n          message: '已取消删除'\n        })\n      })\n    },\n    handleSizeChange(limit) {\n      this.pageSize = limit\n      this.getUserList()\n    },\n    AddForm() {\n      this.dialogFormVisible = true\n      this.form = {\n        username: '',\n        password: '',\n        usertype: '',\n        realname: '',\n        desc: '',\n        studentno: ''\n      }\n    },\n    showInfo(row) {\n      this.dialogFormVisible = true\n      this.form = {\n        username: row.username,\n        realname: row.realname,\n        studentno: row.studentno\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n</style>\n"]}]}