}\n\n.robot .eyeright {\n  box-shadow: 18px 18px 30px rgba(0, 0, 0, 0.1),\n  -18px -18px 30px rgba(255, 255, 255, 1);\n  position: absolute;\n  top: 30%;\n  right: 25%;\n  float: left;\n  background-color: black;\n  width: 4em;\n  height: 40%;\n  border-radius: 2em;\n  animation: blink 5s;\n  -moz-animation: blink 5s; /* Firefox */\n  -webkit-animation: blink 5s; /* Safari and Chrome */\n  -o-animation: blink 5s; /* Opera */\n  -webkit-animation-iteration-count: infinite;\n  animation-iteration-count: infinite;\n}\n\n@keyframes blink {\n  47% {\n    height: 40%;\n    top: 30%;\n    width: 4em;\n  }\n  50% {\n    height: 1%;\n    top: 50%;\n    width: 6em;\n  }\n  53% {\n    height: 40%;\n    top: 30%;\n    width: 4em;\n  }\n}\n\n.hello {\n  position: absolute;\n  top: 15%;\n  right: 40px;\n  line-height: 250%;\n  font-size: 1.1vw;\n}\n\n/*\n.hello p{\n    text-shadow: 5px 5px 15px rgba(0, 0, 0, 0.3),\n    -5px -5px 5px rgba(255, 255, 255, 1);\n}*/\n.question-form {\n  position: absolute;\n  top: 53%;\n  width: 15%;\n  height: 6%;\n  left: 42.5%;\n  transition: 0.5s;\n}\n\n.question {\n  position: absolute;\n  border: 0;\n  width: 100%;\n  background-color: #efeeee;\n  outline: none;\n  height: 83%;\n  font-size: 25px;\n}\n\n.question-line {\n  box-shadow: 5px 5px 30px 20px rgba(0, 0, 0, 0.1),\n  -5px -5px 30px 10px rgba(255, 255, 255, 1);\n  position: relative;\n  top: 100%;\n  width: 100%;\n  height: 5px;\n  background-color: black;\n}\n\n.nav-bar {\n  display: flex;\n  justify-content: space-between;\n  position: absolute;\n  bottom: 10%;\n  width: 80%;\n  height: 7rem;\n  left: 10%;\n}\n\n.nav-box {\n  box-shadow: 18px 18px 30px rgba(0, 0, 0, 0.1),\n  -18px -18px 30px rgba(255, 255, 255, 1);\n  border-radius: 20px;\n  display: flex;\n  justify-content: space-evenly;\n  transition: box-shadow .2s ease-out;\n  float: left;\n  height: 100%;\n  width: 20%;\n  background-color: #efeeee;\n  text-align: center;\n  line-height: 7rem;\n  animation: show 2s;\n  -moz-animation: show 2s; /* Firefox */\n  -webkit-animation: show 2s; /* Safari and Chrome */\n  -o-animation: show 2s; /* Opera */\n  cursor: pointer;\n}\n\n.nav-box a {\n  width: 100%;\n  height: 100%;\n  margin: 0;\n  text-decoration: none;\n  color: black;\n}\n\n@keyframes show {\n  0% {\n    opacity: 0;\n  }\n}\n\n.nav-box:active {\n  box-shadow: 0px 0px 0px rgba(0, 0, 0, 0.2),\n  0px 0px 0px rgba(255, 255, 255, 0.8),\n  inset 18px 18px 18px rgba(0, 0, 0, 0.1),\n  inset -18px -18px 18px rgba(255, 255, 255, 1);\n  transition: box-shadow .2s ease-out;\n}\n\n.nav-box p {\n  margin: 0;\n}\n</style>\n"]}]}