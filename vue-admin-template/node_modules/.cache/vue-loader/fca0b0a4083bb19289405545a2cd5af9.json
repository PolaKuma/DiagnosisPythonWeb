n\n<script>\nexport default {\n  // props: [\"fileType\", \"file\"],\n  props: {\n    fileType: Number,\n    file: File,\n    default() {\n      return {};\n    },\n  },\n  watch: {\n    file() {\n      console.log(this.file);\n    },\n  },  \n  mounted() {\n    console.log(this.file);\n    console.log(this.fileType);\n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.file-card {\n  width: 250px;\n  height: 100px;\n  background-color: rgb(45, 48, 63);\n  border-radius: 20px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  padding: 10px;\n  box-sizing: border-box;\n  cursor: pointer;\n  &:hover {\n    background-color: rgb(33, 36, 54);\n  }\n  img {\n    width: 60px;\n    height: 60px;\n  }\n  .word {\n    width: 60%;\n    margin-left: 10px;\n    overflow: hidden;\n    span {\n      width: 90%;\n      display: inline-block;\n      color: #fff;\n    }\n    span:first-child {\n      font-size: 14px;\n      overflow: hidden;\n      white-space: nowrap;\n      text-overflow: ellipsis;\n    }\n    span:last-child {\n      font-size: 12px;\n      color: rgb(180, 180, 180);\n    }\n  }\n}\n</style>"]}]}